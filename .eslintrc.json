{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "babelOptions": {
      "rootMode": "upward"
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "mjs": "always"
      }
    ]
  }
}
